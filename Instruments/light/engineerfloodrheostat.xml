<?xml version="1.0"?>

<PropertyList>

<path>engineerfloodrheostat.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>Rheostat</object-name>
  <property>controls/lighting/internal/engineer/flood</property>
  <factor>240</factor>
  <offset-deg>-30</offset-deg>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>Rheostat</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-adjust</command>
    <property>controls/lighting/internal/engineer/flood</property>
    <step>0.2</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>instrumentation/generic/click</property>
   </binding>
  </action>
  <action>
   <button>1</button>
   <binding>
    <command>property-adjust</command>
    <property>controls/lighting/internal/engineer/flood</property>
    <step>-0.2</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>instrumentation/generic/click</property>
   </binding>
  </action>
 </animation>

</PropertyList>

