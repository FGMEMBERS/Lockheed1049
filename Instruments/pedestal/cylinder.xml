<!-- propeller -->

 <animation>
  <type>rotate</type>
  <object-name>MasterPropeller</object-name>
  <property>controls/engines/engine[0]/propeller-pitch</property>
  <factor>-75</factor>
  <offset-deg>85</offset-deg>   
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>MasterPropeller</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <step>0.1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[1]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[2]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[3]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
  </action>
  <action>
   <button>1</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/propeller-pitch</property>
