<?xml version="1.0"?>

<!-- rotation scope is adjusted so that :

     - overhead light switch is always visible by Captain.
     - landing gear lights are always visible by Captain.
-->


<PropertyList>

<path>cylinder.ac</path>

<!-- elevator -->

 <animation>
  <type>rotate</type>
  <object-name>ElevatorTabLW</object-name>
  <property>controls/flight/elevator-trim</property>
  <factor>-1080</factor>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>ElevatorTabLW</object-name>
  <action>
   <button>0</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/flight/elevator-trim</property>
    <step>0.003</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
  </action>
  <action>
   <button>1</button>
   <repeatable>false</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/flight/elevator-trim</property>
    <step>-0.003</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
  </action>
 </animation>

