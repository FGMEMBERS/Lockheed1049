<?xml version="1.0"?>

<!--
     Credits :
     =======
     - adapted (x 0.7 scale) from G. Neely's Lockheed 1049-H.
-->


<PropertyList>

<path>airtemp.ac</path>

<animation>
  <type>material</type>
  <object-name>Airtemp.FaceBack</object-name>
  <object-name>Airtemp.Needle</object-name>
  <emission>
    <red>0.81</red>
    <green>0.40</green>
    <blue>0.20</blue>
    <factor-prop>systems/lighting/internal/engineer/instrument-light</factor-prop>
  </emission>
  <ambient>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>systems/lighting/internal/ambient-light</factor-prop>
  </ambient>
</animation>

<animation>
  <type>rotate</type>
  <object-name>Airtemp.Needle</object-name>
  <property>environment/temperature-degc</property>
  <interpolation>
    <entry><ind>-60</ind><dep>-60</dep></entry>
    <entry><ind>  0</ind><dep>  0</dep></entry>
    <entry><ind> 60</ind><dep> 60</dep></entry>
  </interpolation>
  <center>
    <x-m>0</x-m>
    <y-m>0</y-m>
    <z-m>-0.016</z-m>
  </center>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
</animation>

</PropertyList>
