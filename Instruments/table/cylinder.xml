<?xml version="1.0"?>

<PropertyList>

<path>cylinder.ac</path>

 <animation>
  <name>Cylinder</name>
  <object-name>ControlQuadrant</object-name>
  <object-name>PropellerEngineer</object-name>
  <object-name>ThrottleEngineer1</object-name>
  <object-name>ThrottleEngineer2</object-name>
  <object-name>ThrottleEngineer3</object-name>
  <object-name>ThrottleEngineer4</object-name>
  <object-name>EngineBlower1</object-name>
  <object-name>EngineBlower2</object-name>
  <object-name>EngineBlower3</object-name>
  <object-name>EngineBlower4</object-name>
  <object-name>Mixture1</object-name>
  <object-name>Mixture2</object-name>
  <object-name>Mixture3</object-name>
  <object-name>Mixture4</object-name>
  <object-name>FuelTankSelector1</object-name>
  <object-name>FuelTankSelector2</object-name>
  <object-name>FuelTankSelector3</object-name>
  <object-name>FuelTankSelector4</object-name>
 </animation>

 <animation>
  <type>material</type>
  <object-name>Cylinder</object-name>
  <condition>
   <or>
    <not>
     <property>/sim/rendering/rembrandt/enabled</property>
    </not>
    <less-than-equals>
     <property>/sim/rendering/shaders/lights</property>
     <property n="1">controls/environment/rembrandt/panel</property>
    </less-than-equals>
   </or>
  </condition>
  <emission>
   <red>1.0</red>
   <green>0.1</green>
   <blue>0.1</blue>
   <factor-prop>systems/lighting/internal/engineer/flood-light</factor-prop>
  </emission> 
 </animation>

<!-- propeller pitch -->

 <animation>
  <type>rotate</type>
  <object-name>PropellerEngineer</object-name>
  <property>controls/engines/engine[0]/propeller-pitch</property>
  <factor>-70</factor>
  <offset-deg>35</offset-deg>   
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>PropellerEngineer</object-name>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <step>0.1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-assign</command>
    <property>controls/engines/engine[1]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[2]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[3]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
  </action>
  <action>
   <button>1</button>
   <repeatable>true</repeatable>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[0]/propeller-pitch</property>
    <step>-0.1</step>
    <min>0</min>
    <max>1</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[1]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[2]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
   <binding>
    <command>property-adjust</command>
    <property>controls/engines/engine[3]/propeller-pitch</property>
    <property n="1">controls/engines/engine[0]/propeller-pitch</property>
   </binding>
  </action>
 </animation>

