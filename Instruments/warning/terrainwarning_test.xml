<?xml version="1.0"?>

<PropertyList>

<path>terrainwarning_test.ac</path>

 <animation>
  <type>material</type>
  <object-name>TerrainTest</object-name>
  <object-name>TerrainAltitude</object-name>
  <emission>
    <red>0.81</red>
    <green>0.40</green>
    <blue>0.20</blue>
    <factor-prop>systems/lighting/internal/panel-light</factor-prop>
  </emission>
  <ambient>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
    <factor-prop>systems/lighting/internal/ambient-light</factor-prop>
  </ambient>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>TerrainAltitude</object-name>
  <action>
   <button>0</button>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/terrain-warning/selector</property>
    <step>1</step>
    <min>0</min>
    <max>2</max>
    <wrap>false</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>instrumentation/generic/click</property>
   </binding>
  </action>
  <action>
   <button>1</button>
   <binding>
    <command>property-adjust</command>
    <property>/instrumentation/terrain-warning/selector</property>
    <step>-1</step>
    <min>0</min>
    <max>2</max>
    <wrap>false</wrap>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>instrumentation/generic/click</property>
   </binding>
  </action>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>TerrainAltitude</object-name>
  <property>/instrumentation/terrain-warning/selector</property>
  <factor>-45</factor>
  <offset-deg>45</offset-deg>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.018</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>

