<?xml version="1.0"?>

<PropertyList>

<path>taxi.ac</path>

 <animation>
  <type>noshadow</type>
  <object-name>TaxiLight</object-name>
 </animation>

 <animation>
  <type>material</type>
  <object-name>TaxiLight</object-name>
  <emission>
   <red>1.0</red>
   <green>1.0</green>
   <blue>1.0</blue>
   <factor-prop>instrumentation/generic/factor</factor-prop>
  </emission> 
 </animation>

 <animation>
  <type>select</type>
  <object-name>TaxiLight</object-name>
  <condition>
   <and>
    <property>controls/lighting/external/taxi/on</property>
    <not>
     <property>controls/lighting/external/taxi/passing</property>
    </not>
   </and>
  </condition>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>TaxiLight</object-name>
  <factor>0.0</factor>
  <offset-deg>-45.0</offset-deg>
  <center>
   <x-m>-17.07</x-m>
   <y-m>0.0</y-m>
   <z-m>-0.510</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>TaxiLight</object-name>
  <offset-m>-17.07</offset-m>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>TaxiLight</object-name>
  <offset-m>-0.510</offset-m>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>

</PropertyList>

