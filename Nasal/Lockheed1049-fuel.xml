<?xml version="1.0"?>

<!-- Fuel configurations.

     Default is max landing weight.

     Center 4 is emptied first, then middle tanks 0 & 3; inboards 1 & 2 and outboards 5 & 6 feeds engines.


     Warning : the nasal script doesn't work, if the tanks are not ordered in the FDM XML file !
-->

<PropertyList>

   <tanks>

<!-- Max landing weight (98500 lb), -->

    <filling n="0">
     <comment>max landing</comment>
     <tank n="0">
      <level-gal_us>120.0</level-gal_us>
     </tank>
     <tank n="1"></tank>
     <tank n="2"></tank>
     <tank n="3">
      <level-gal_us>120.0</level-gal_us>
     </tank>
     <tank n="4">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="5"></tank>
     <tank n="6"></tank>
    </filling>

<!-- Max takeoff weight (120000 lb). -->

    <filling n="1">
     <comment>max takeoff</comment>
    </filling>

<!-- Empty tanks (center is emptied first) : for FDM test. -->

    <filling n="2">
     <comment>empty</comment>
     <tank n="0">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="1">
      <level-gal_us>85.0</level-gal_us>
     </tank>
     <tank n="2">
      <level-gal_us>85.0</level-gal_us>
     </tank>
     <tank n="3">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="4">
      <level-gal_us>0.0</level-gal_us>
     </tank>
     <tank n="5">
      <level-gal_us>85.0</level-gal_us>
     </tank>
     <tank n="6">
      <level-gal_us>85.0</level-gal_us>
     </tank>
    </filling>

<!-- other -->

   </tanks>

</PropertyList>


