<?xml version="1.0"?>

<!-- Label on cross feed lever -->

<PropertyList>
 <name>Cross feed</name>
 <w-base>20</w-base>
 <h-base>300</h-base>

 <layers>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>15</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>CROSS OPEN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>rotation</type>
     <offset>90</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
